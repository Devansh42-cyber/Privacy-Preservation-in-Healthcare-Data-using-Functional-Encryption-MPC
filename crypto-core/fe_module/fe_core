PRIME = 2**127 - 1
def mod_p(x):
    return x % PRIME
def vector_to_list(patient):
    return [
        patient["age"],
        patient["gender"],
        patient["disease"],
        patient["blood_pressure"],
        patient["risk_score"]
    ]

def inner_product(vec1, vec2):
    return mod_p(sum(a*b for a, b in zip(vec1, vec2)))